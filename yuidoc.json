{
  "name": "Ember Stylish Buttons",
  "description": "Some fresh styles & hover effects for the modern Ember button.",
  "url": "https://github.com/rajasegar/ember-stylish-buttons",
  "version": "0.0.7",
  "options": {
    "enabledEnvironments": ["development","production"],
    "paths": [
      "addon"
    ],
    "themedir": "node_modules/yuidoc-ember-theme",
    "helpers": ["node_modules/yuidoc-ember-theme/helpers/helpers.js"],
    "exclude": "vendor",
    "outdir": "docs",
    "linkNatives": true,
    "quiet": true,
    "parseOnly": false,
    "lint": false
  }
}
